{
  "name": "hexad-assignment",
  "version": "0.1.1",
  "private": true,
  "homepage": "http://thomazcapra.github.io/hexad-assignment",
  "scripts": {
    "build": "react-scripts build",
    "deploy": "react-scripts build && gh-pages -d build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "coverage": "yarn run test -- --coverage --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80
      }
    },
    "collectCoverageFrom": [
      "**/src/**/*.{ts,tsx}",
      "!**/src/*.{js,d.ts,d.js}",
      "!**/src/**/index.{ts,tsx}"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-test-renderer": "^16.9.2",
    "gh-pages": "^2.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "react-test-renderer": "^16.13.0",
    "rxjs": "^6.5.4",
    "typescript": "~3.7.2"
  }
}